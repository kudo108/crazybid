<h1>Listing user_bids</h1>
<%if session["warden.user.user.key"][1][0]==1%>
<table>
  <tr>
    <th>Time</th>
    <th>User price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_bids.each do |user_bid| %>
  <tr>
    <td><%= user_bid.time %></td>
    <td><%= user_bid.user_price %></td>
    <td><%= link_to 'Show', user_bid %></td>
    <td><%= link_to 'Edit', edit_user_bid_path(user_bid) %></td>
    <td><%= link_to 'Destroy', user_bid, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%end%>
<%= link_to 'New User bid', new_user_bid_path %>
