<% if 1==0%>
<%= form_for(@user_bid) do |f| %>
  <% if @user_bid.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user_bid.errors.count, "error") %> prohibited this @user_bid from being saved:</h2>

      <ul>
      <% @user_bid.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<%if session["warden.user.user.key"][1][0]==1%>
